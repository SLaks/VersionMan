{
  "manifest_version": 2,

  "name": "VersionMan",
  "description": "This extension makes sure you're always reading the correct version of developer documentation.",
  "version": "1.0",

  "permissions": [
    "storage",
    "chrome://favicon/",

    "http://nodejs.org/docs",
    "http://docs.oracle.com/javase",
    "http://msdn.microsoft.com/"
  ],
  "background" : {
    "scripts": ["js/bower_components/jquery/jquery.js", "js/services.js", "js/navigation-listener.js"],
    "persistent": false
  },

  "browser_action": {
    "default_icon": "img/icon.png",
    "default_popup": "popup.html"
  }
}